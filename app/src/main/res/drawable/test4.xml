<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Irish">

        <item android:state_pressed="true" >
            <shape>
                <solid
                    android:color="#1B5E20" />
                <stroke
                    android:width="2dp"
                        android:color="#A5D6A7" />
                <corners
                    android:radius="7dp" />
            </shape>
        </item>
        <item>
            <shape>
                <gradient
                    android:startColor="#A5D6A7"
                    android:endColor="#4CAF50"
                    android:angle="270" />
                <stroke
                    android:width="2dp"
                    android:color="#A5D6A7" />
                <corners
                    android:radius="7dp" />

            </shape>
        </item>
</selector>